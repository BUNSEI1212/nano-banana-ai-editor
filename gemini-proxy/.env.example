# Gemini Proxy Service Configuration

# Relay API Configuration (替换原有的Gemini直连配置)
USE_RELAY_API=true
RELAY_API_KEY=your_relay_api_key_here
RELAY_API_URL=https://hiapi.online/v1

# Server Configuration
PORT=3001
NODE_ENV=production
HOST=0.0.0.0

# Database Configuration
DATABASE_PATH=./data/proxy.db

# JWT Configuration
JWT_SECRET=your_jwt_secret_here_change_this_in_production

# Casdoor Configuration
CASDOOR_ENDPOINT=https://your-casdoor-instance.com
CASDOOR_CLIENT_ID=your_client_id
CASDOOR_CLIENT_SECRET=your_client_secret
CASDOOR_ORGANIZATION=your_organization
CASDOOR_APPLICATION=your_application

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/proxy.log

# CORS Configuration
CORS_ORIGIN=*

# Security
HELMET_ENABLED=true

# Performance
MAX_REQUEST_SIZE=50mb
